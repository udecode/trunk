{
  "name": "trunk",
  "version": "0.0.1",
  "description": "A tree data structure library inspired by Slate",
  "keywords": [
    "tree",
    "data-structure",
    "slate"
  ],
  "bugs": {
    "url": "https://github.com/udecode/trunk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/udecode/trunk.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "brl": "sh tooling/brl.sh",
    "build": "tsdown --config tooling/tsdown.config.ts",
    "build:watch": "tsdown --config tooling/tsdown.config.ts --watch",
    "check": "pnpm run lint && pnpm run build && pnpm run typecheck",
    "lint": "biome check && eslint",
    "lint:fix": "biome check --write",
    "release": "bun run build && bun changeset publish",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "immer": "^10.1.1",
    "is-hotkey": "^0.2.0",
    "is-plain-object": "^5.0.0",
    "jotai-x": "^2.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@changesets/cli": "2.29.7",
    "@rollup/plugin-babel": "^6.1.0",
    "@types/is-hotkey": "^0.1.10",
    "@types/lodash": "^4.17.16",
    "@types/node": "^24.7.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/parser": "^8.47.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "barrelsby": "^2.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "lefthook": "^2.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsdown": "^0.16.6",
    "typescript": "5.9.3",
    "ultracite": "6.3.6"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "packageManager": "bun@1.3.3",
  "publishConfig": {
    "access": "public"
  }
}
